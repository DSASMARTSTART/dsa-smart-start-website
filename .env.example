# DSA Smart Start Environment Variables
# ======================================
# Copy this file to .env and fill in your values

# Admin Configuration
VITE_ADMIN_EMAIL=your-admin-email@example.com

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Raiffeisen RaiAccept Payment Configuration (Card Payments)
# Set RAIACCEPT_API_USERNAME and RAIACCEPT_API_PASSWORD as Supabase Edge Function secrets:
#   supabase secrets set RAIACCEPT_API_USERNAME=your_username RAIACCEPT_API_PASSWORD=your_password
VITE_RAIACCEPT_ENABLED=true

# PayPal Configuration
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id
VITE_PAYPAL_MODE=sandbox

# Stripe Configuration (if used)
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key
