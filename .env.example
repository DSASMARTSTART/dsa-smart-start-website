# DSA Smart Start Environment Variables
# ======================================
# Copy this file to .env and fill in your values

# Admin Configuration
VITE_ADMIN_EMAIL=your-admin-email@example.com

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Raiffeisen/Nestpay Payment Configuration (Card Payments)
# NOTE: STORE_KEY should be set ONLY in Supabase Edge Function secrets, NOT here!
# Set it in: Supabase Dashboard → Edge Functions → generate-payment-hash → Secrets
VITE_RAIFFEISEN_MERCHANT_ID=your_merchant_id
VITE_RAIFFEISEN_TERMINAL_ID=your_terminal_id
VITE_RAIFFEISEN_API_URL=https://entegrasyon.asseco-see.com.tr/fim/est3Dgate

# PayPal Configuration
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id
VITE_PAYPAL_MODE=sandbox

# Stripe Configuration (if used)
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key
